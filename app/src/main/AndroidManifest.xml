<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.br.villasfood">

    <application
        android:allowBackup="true"
        android:icon="@drawable/preview"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".AppApplication">
        <activity android:name=".ui.activity.cart.ListCartActivity"/>
        <activity android:name=".ui.activity.food.FoodResumeActivity"/>
        <activity android:name=".ui.activity.wine.WineResumeActivity"/>
        <activity android:name=".ui.activity.sodas.SodasResumeActivity"/>
        <activity android:name=".ui.activity.wine.ListWineActivity"/>
        <activity android:name=".ui.activity.sodas.ListSodasActivity"/>
        <activity android:name=".ui.activity.order.FinishOrderActivity"/>
        <activity android:name=".ui.activity.food.ListFoodActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>